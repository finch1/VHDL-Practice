FUNCTION alt_u_div_53f (denominator[6..0], numerator[6..0])
RETURNS ( quotient[6..0], remainder[6..0]);

--synthesis_resources = lut 52 
SUBDESIGN sign_div_unsign_76h
( 
	denominator[6..0]	:	input;
	numerator[6..0]	:	input;
	quotient[6..0]	:	output;
	remainder[6..0]	:	output;
) 
VARIABLE 
	divider : alt_u_div_53f;
	adder_result_int[7..0]	:	WIRE;
	adder_cin	:	WIRE;
	adder_dataa[6..0]	:	WIRE;
	adder_datab[6..0]	:	WIRE;
	adder_result[6..0]	:	WIRE;
	compl_adder1_result_int[7..0]	:	WIRE;
	compl_adder1_cin	:	WIRE;
	compl_adder1_dataa[6..0]	:	WIRE;
	compl_adder1_datab[6..0]	:	WIRE;
	compl_adder1_result[6..0]	:	WIRE;
	compl_adder_4_result_int[7..0]	:	WIRE;
	compl_adder_4_cin	:	WIRE;
	compl_adder_4_dataa[6..0]	:	WIRE;
	compl_adder_4_datab[6..0]	:	WIRE;
	compl_adder_4_result[6..0]	:	WIRE;
	adder_out[6..0]	: WIRE;
	den_choice[6..0]	: WIRE;
	gnd_wire	: WIRE;
	neg_num[6..0]	: WIRE;
	neg_quot[6..0]	: WIRE;
	norm_num[6..0]	: WIRE;
	num_choice[6..0]	: WIRE;
	pre_neg_den[6..0]	: WIRE;
	pre_neg_quot[6..0]	: WIRE;
	pre_quot[6..0]	: WIRE;
	protect_quotient[6..0]	: WIRE;
	protect_remainder[6..0]	: WIRE;
	q_is_neg	: WIRE;
	vcc_wire	: WIRE;
	zero_wire[6..0]	: WIRE;
	zero_wire_3w[6..0]	: WIRE;

BEGIN 
	divider.denominator[] = den_choice[];
	divider.numerator[] = norm_num[];
	adder_result_int[] = (adder_dataa[], 0) - (adder_datab[], !adder_cin);
	adder_result[] = adder_result_int[7..1];
	adder_cin = gnd_wire;
	adder_dataa[] = den_choice[];
	adder_datab[] = protect_remainder[];
	compl_adder1_result_int[] = (compl_adder1_dataa[], compl_adder1_cin) + (compl_adder1_datab[], compl_adder1_cin);
	compl_adder1_result[] = compl_adder1_result_int[7..1];
	compl_adder1_cin = vcc_wire;
	compl_adder1_dataa[] = (! denominator[]);
	compl_adder1_datab[] = zero_wire[];
	compl_adder_4_result_int[] = (compl_adder_4_dataa[], compl_adder_4_cin) + (compl_adder_4_datab[], compl_adder_4_cin);
	compl_adder_4_result[] = compl_adder_4_result_int[7..1];
	compl_adder_4_cin = vcc_wire;
	compl_adder_4_dataa[] = (! pre_quot[]);
	compl_adder_4_datab[] = zero_wire_3w[];
	adder_out[] = adder_result[];
	den_choice[] = ((denominator[] & (! denominator[6..6])) # (pre_neg_den[] & denominator[6..6]));
	gnd_wire = B"0";
	neg_num[] = (! num_choice[]);
	neg_quot[] = (! protect_quotient[]);
	norm_num[] = ((num_choice[] & (! num_choice[6..6])) # (neg_num[] & num_choice[6..6]));
	num_choice[] = numerator[];
	pre_neg_den[] = compl_adder1_result[];
	pre_neg_quot[] = compl_adder_4_result[];
	pre_quot[] = ((protect_quotient[] & (! num_choice[6..6])) # (neg_quot[] & num_choice[6..6]));
	protect_quotient[] = divider.quotient[];
	protect_remainder[] = divider.remainder[];
	q_is_neg = denominator[6..6];
	quotient[] = ((pre_quot[] & (! q_is_neg)) # (pre_neg_quot[] & q_is_neg));
	remainder[] = ((protect_remainder[] & (! num_choice[6..6])) # (adder_out[] & num_choice[6..6]));
	vcc_wire = B"1";
	zero_wire[] = B"0000000";
	zero_wire_3w[] = B"0000000";
END;
